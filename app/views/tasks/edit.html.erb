<h1>Edit Task</h1>
<%= form_with(model: @task, url: task_path(@task), method: :patch) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :details %>
  <%= form.text_area :details %>

  <%= form.label :complete %>
  <%= form.check_box :complete %>

  <%= form.submit "Update Task" %>
<% end %>
